[
    {
        "settings": [ "master" ],
	    "cacheViewTemplates": false,
	    "specs": {
            "_frame": {
                "type": "HTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "assets": {
                        "top": {
                            "blob": ["<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">"],
                            "css": ["/static/Frame/assets/reset.css", "/static/Frame/assets/global.css"]
                        }
                    }
                }
            },

            "rider": {
                "base": "_frame",
                "config": {
                    "title": "B.lock",
                    "child": {
                        "type": "Rider",
                        "config": {
                            "assets": {
                                "top": {
                                    "css": ["/static/Rider/assets/rider.css"]
                                }
                            },
	                        "children": {
                                "topNav": {"type": "TopNav"},
		                        "subNav": {"type":"RiderNav"},
		                        "riderMap": {"type": "RiderMap"}
                            }
                        }
                    }
                }
            },

		    "lender": {
                "base": "_frame",
                "config": {
                    "title": "B.lock",
                    "child": {
                        "type": "Lender",
                        "config": {
                            "assets": {
                                "top": {
                                    "css": ["/static/Lender/assets/lender.css"]
                                }
                            },
                            "children": {
                                "topNav": {"type": "TopNav"},
	                            "subNav": {"type":"LenderNav"},
                                "lenderMap": {"type": "LenderMap"}
                            }
                        }
                    }
                }
            }
	    }
    },
    {
        "staticHandling": {
            "forceUpdate": true
        }
    }
]
